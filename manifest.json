{
  "name": "link2links",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/big.png",
    "48": "icons/big.png",
    "128": "icons/big.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/main.png",
    "default_title": "Copy URLs"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "tabs"
    
  ],
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Toggle feature foo"
    }
  }
  
}